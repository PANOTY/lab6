#include <stdio.h>

int main() {
    int start, end;

    // รับค่าจำนวนเริ่มต้นและจำนวนสิ้นสุดจากผู้ใช้
    printf("Start:\n");
    scanf("%d", &start);
    printf("End:\n");
    scanf("%d", &end);

    // ใช้ for loop เพื่อแสดงแม่สูตรคูณจาก start ถึง end
    for (int i = start; i <= end; i++) {
        for (int j = 1; j <= 9; j++) {
            printf("%d x %d = %d\n", i, j, i * j);
        }
        printf("\n"); // เพื่อเพิ่มบรรทัดว่างระหว่างสูตรคูณ
    }

    return 0;
}
