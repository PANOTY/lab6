#include <stdio.h>

int isPrime(int num) {
    if (num < 2) return 0; // เลขน้อยกว่า 2 ไม่เป็นจำนวนเฉพาะ
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) return 0; // หากหารลงตัว ไม่เป็นจำนวนเฉพาะ
    }
    return 1; // เป็นจำนวนเฉพาะ
}

int main() {
    int n;
    printf("Enter number :\n");
    scanf("%d", &n);

    printf("Output:\n");
    // ใช้ for loop เพื่อหาจำนวนเฉพาะจาก n ลงมาจนถึง 2
    for (int i = n; i >= 2; i--) {
        if (isPrime(i)) {
            printf("%d ", i);
        }
    }
    printf("\n"); // ลงบรรทัดใหม่หลังจากเสร็จสิ้นการพิมพ์
    return 0;
}
